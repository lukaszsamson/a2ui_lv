{
  "name": "Claude A2UI Agent",
  "description": "A2A-compliant agent that generates A2UI interfaces using Claude",
  "url": "http://localhost:3002",
  "version": "1.0.0",
  "capabilities": {
    "extensions": [
      {
        "uri": "https://a2ui.org/a2a-extension/a2ui/v0.8",
        "params": {
          "supportedCatalogIds": [
            "https://a2ui.org/standard-catalog/v0.8/main.json",
            "https://a2ui.org/catalogs/v0.8.json",
            "https://a2ui.org/catalogs/standard/v0.8.json"
          ],
          "acceptsInlineCatalogs": false
        }
      }
    ],
    "streaming": true,
    "pushNotifications": false
  },
  "authentication": {
    "schemes": []
  },
  "defaultInputModes": ["text"],
  "defaultOutputModes": ["text"]
}
